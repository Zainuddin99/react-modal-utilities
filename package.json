    {
      "path": "dist/react-modal-utility.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^7.0.8",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "cz-conventional-changelog": "^3.3.0",
    "husky": "^8.0.1",
    "postcss": "^8.4.14",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rollup-plugin-postcss": "^4.0.2",
    "semantic-release": "^19.0.3",
    "size-limit": "^7.0.8",
    "tsdx": "^0.14.1",
    "tslib": "^2.4.0",
    "typescript": "^4.7.4"
  },
  "dependencies": {},
  "repository": {
    "type": "git",
    "url": "https://github.com/Zainuddin99/react-modal-utility.git"
  },
